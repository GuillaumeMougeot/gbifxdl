{
    "creator": "gmougeot", 
    "notificationAddresses": [
        "gmo@ecos.au.dk"
    ],
    "sendNotification": true,
    "format": "DWCA",
    "predicate": {
        "type": "and",
        "predicates": [
            {
                "type": "equals",
                "key": "TAXON_KEY",
                "value": "797"  
            },
            {
                "type": "equals",
                "key": "MEDIA_TYPE",
                "value": "StillImage"
            },
            {
                "type": "in",
                "key": "COUNTRY",
                "values": [
                    "AL", "AD", "AT", "AM", "BY", "BE", "BA", "BG", "HR", "CY",
                    "CZ", "CH", "DK", "EE", "FI", "FR", "FO", "DE", "GR", "GE", 
                    "GI", "HU", "IS", "IE", "IT", "IM", "LV", "LI", "LT", "LU", 
                    "MT", "MD", "MC", "ME", "NL", "MK", "NO", "PL", "PT", "RO", 
                    "RU", "RS", "SM", "RS", "SK", "SI", "ES", "SE", "CH", "TR", 
                    "UA", "GB", "VA", "XK"
                ]
            },
            {
                "type": "in",
                "key": "BASIS_OF_RECORD",
                "value": [
                    "LIVING_SPECIMEN", "OBSERVATION", "HUMAN_OBSERVATION", "MACHINE_OBSERVATION", "OCCURRENCE"
                ]
            },
            {
                "type": "equals",
                "key": "YEAR",
                "value": "2017"
            }
        ]
    }
}